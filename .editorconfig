# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.py]
charset = utf-8

# 2 space indentation (TF style)
[*.py]
indent_style = space
indent_size = 2

[{.travis.yml}]
indent_style = space
indent_size = 2
